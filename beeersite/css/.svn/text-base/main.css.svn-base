}

.sq_radio_selected
{
	background: transparent url(../i/sq_radio_selected.png) no-repeat top left;
}

.sq_radio_unselected
{
	background: transparent url(../i/sq_radio_unselected.png) no-repeat top left;
}

h8
{
	color: #bfbfb9;
	font-weight: 300;
}

#beeer_detail_wheel
{
