}

#beeer_detail_serving
{
	padding: 0 0 0 2px;
	position: relative;
	text-align: center;
}

#beeer_detail_serving_bottle, #beeer_detail_serving_can, #beeer_detail_serving_draft, #beeer_detail_serving_growler, #beeer_detail_serving_cask
{
	
	position: relative;
	float: left;
	height: 70px;
